{
    // 本文件主要定义了通用 UI 的行为和显示内容
    // 可参考文档和示例添加更多您需要的功能
    // 文档：https://github.com/MaaXYZ/MaaFramework/blob/main/docs/zh_cn/3.3-ProjectInterfaceV2%E5%8D%8F%E8%AE%AE.md
    // 示例：https://github.com/MaaXYZ/MaaFramework/blob/main/sample/interface.json
    "interface_version": 2,
    "name": "MaaXXX",
    "description": "这是一个使用 MaaFramework 创建的新项目。",
    "contact": "QQ群: 123456789",
    "license": "MIT",
    "welcome": "首次使用！欢迎使用 MAA!",
    // 填写项目地址和版本号可实现自动更新功能
    "github": "https://github.com/MaaXYZ/MaaPracticeBoilerplate",
    "version": "1.0.0",
    // Mirror酱为第三方分发服务，新项目默认关闭
    // 若有需要请联系 Mirror酱 开通，直接自行开启是无法使用的
    // "mirrorchyan_rid": "MaaXXX",
    // "mirrorchyan_multiplatform": true,
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb",
            "display_short_side": 720 // 默认缩放分辨率的短边长度
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "task": [
        {
            "name": "0.进入游戏",
            "entry": "进入游戏",
            //TODO 异常情况 可能存在更新
            "default_check": true
        },
        {
            "name": "1.领取委托",
            "entry": "领取委托",
            "default_check": true
        },
        {
            "name": "2.领取采购奖励",
            "entry": "领取采购奖励",
            "default_check": true
        },
        {
            "name": "3.送礼物邀约",
            "entry": "送礼物邀约",
            "default_check": true
        },
        {
            "name": "4.代理人互动",
            "entry": "代理人互动",
            "default_check": true
        },
        {
            "name": "5.领取好友体力",
            "entry": "领取好友体力",
            "default_check": true
        },
        {
            "name": "5.活动刷体力",
            "entry": "活动刷体力",
            "default_check": true
        },
        {
            "name": "6.常规刷体力",
            "entry": "常规刷体力",
            "option": [
                "试炼类型"
            ]
        },
        {
            "name": "7.每日奖励",
            "entry": "每日奖励",
            "default_check": true
        },
        {
            "name": "8.领取大月卡",
            "entry": "基金-领取大月卡",
            "default_check": true
        }
    ],
    "option": {
        "试炼类型": {
            "type": "select",
            "label": "选择试炼类型",
            "description": "选择要刷的试炼关卡类型",
            "default_case": "基础试炼",
            "cases": [
                {
                    "name": "基础试炼",
                    "label": "基础试炼",
                    "description": "刷基础试炼关卡",
                    "pipeline_override": {
                        "常规刷体力-选择试炼类型": {
                            "next": [
                                "常规刷体力-点击基础试炼"
                            ]
                        }
                    }
                },
                {
                    "name": "纹章试炼-帝国卫队",
                    // TODO 还有 地理协会 恩赐意志
                    "label": "纹章试炼",
                    "description": "刷纹章试炼关卡",
                    "pipeline_override": {
                        "常规刷体力-选择试炼类型": {
                            "next": [
                                "常规刷体力-点击纹章试炼"
                            ]
                        }
                    }
                },
                {
                    "name": "技巧试炼-射击游戏卡带",
                    "label": "技巧试炼-射击游戏",
                    "description": "刷技巧试炼关卡-射击游戏卡带",
                    "pipeline_override": {
                        "常规刷体力": {
                            "next": [
                                "常规刷体力-技巧射击-点击悬赏试炼"
                            ]
                        }
                    }
                },
                {
                    "name": "技巧试炼-节奏游戏卡带",
                    "label": "技巧试炼-节奏游戏",
                    "description": "刷技巧试炼关卡-节奏游戏卡带",
                    "pipeline_override": {
                        "常规刷体力": {
                            "next": [
                                "常规刷体力-技巧节奏-点击悬赏试炼"
                            ]
                        }
                    }
                },
                {
                    "name": "技巧试炼-格斗游戏卡带",
                    "label": "技巧试炼-格斗游戏",
                    "description": "刷技巧试炼关卡-格斗游戏卡带",
                    "pipeline_override": {
                        "常规刷体力": {
                            "next": [
                                "常规刷体力-技巧格斗-点击悬赏试炼"
                            ]
                        }
                    }
                }
            ]
        }
    }
}